	<query language="SQL"><![CDATA[SELECT
    u.name AS user,
    p.name AS product,
    sa.quantity,
    sa.notes,
    sa.timestamp
FROM
    stock_adjustments sa
JOIN
    users u ON sa.user_id = u.id
JOIN
    products p ON sa.product_id = p.id
ORDER BY
    sa.id ASC]]></query>
