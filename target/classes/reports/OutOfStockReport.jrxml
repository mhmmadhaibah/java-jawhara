	<query language="SQL"><![CDATA[SELECT 
    s.name AS supplier,
    c.name AS category,
    p.name AS product,
    ps.quantity
FROM 
    products p
JOIN 
    product_stocks ps ON p.id = ps.product_id
JOIN 
    suppliers s ON p.supplier_id = s.id
JOIN 
    categories c ON p.category_id = c.id
WHERE 
    ps.quantity <= 0
ORDER BY 
    ps.quantity ASC, s.id ASC, c.name ASC]]></query>
