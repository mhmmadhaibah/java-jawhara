	<queryString language="SQL">
		<![CDATA[SELECT 
    c.name AS category,
    p.name AS product,
    ps.quantity AS stock_quantity
FROM 
    product_stocks ps
JOIN 
    products p ON ps.product_id = p.id
JOIN 
    categories c ON p.category_id = c.id
WHERE 
    ps.quantity <= 0
ORDER BY 
    ps.quantity ASC]]>
	</queryString>
