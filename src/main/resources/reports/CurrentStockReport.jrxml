	<query language="SQL"><![CDATA[SELECT 
    s.name AS supplier,
    c.name AS category,
    p.name AS product,
    ps.quantity AS quantity
FROM
    product_stocks ps
JOIN
    products p ON ps.product_id = p.id
JOIN
    suppliers s ON p.supplier_id = s.id
JOIN
    categories c ON p.category_id = c.id
ORDER BY
    s.id ASC, c.name ASC]]></query>
